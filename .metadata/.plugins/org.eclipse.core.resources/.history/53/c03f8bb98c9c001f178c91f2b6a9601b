
public class Habileté {
	String nomHabileté;
	String description;
	int score;
	int modificateur;
	
	Habileté(){}
	Habileté(String nomHabileté, String description, int score, int modificateur)
	{
		this.nomHabileté = nomHabileté;
		this.description = description;

	}
	public String getNomHabileté() {
		return nomHabileté;
	}
	public void setNomHabileté(String nomHabileté) {
		this.nomHabileté = nomHabileté;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public int getScore() {
		return score;
	}
	public void setScore(int score) {
		this.score = score;
	}
	public int getModificateur() {
		return modificateur;
	}
	public void setModificateur(int modificateur) {
		this.modificateur = modificateur;
	}
	
	
}
